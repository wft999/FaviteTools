<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <template id="assets_backend" name="web_demo assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/web_demo/static/src/less/tour_dashboard.less"/>
            </xpath>
        </template>
        
        <template id="assets_common" name="tours assets" inherit_id="web.assets_common">
        	<xpath expr="//script[@src='/web_tour/static/src/js/tour_service.js']" position="after">
                <script type="text/javascript" src="/web_demo/static/src/js/tour.js"></script>
            </xpath>
        </template>
</odoo>
