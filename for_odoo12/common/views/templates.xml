<odoo>
    <data>
        <template id="assets_common" name="favite assets" inherit_id="web.assets_common">
        	<xpath expr="." position="inside">
                <script type="text/javascript" src="/common/static/src/js/common.js"></script>
            </xpath>
        </template>
        
        <template id="layout" name="layout" inherit_id="web.layout">
        	<xpath expr="//title" position="replace">
            	<title>Favite Tools</title>
        	</xpath>
    	</template>
    	
    	<template id="layout2" name="layout2" inherit_id="web.login_layout">
   			<xpath expr="//img" position="replace">
            	<img t-attf-src="/web_guide/static/description/logo.png" alt="Logo" style="max-height:120px; max-width: 100%; width:auto"/>
        	</xpath>
        
        	<xpath expr="//a[@href='https://www.odoo.com?utm_source=db&amp;utm_medium=auth']" position="replace">
            	<a href="http://www.favite.com" target="_blank">Powered by <span>Favite</span></a>
        	</xpath>
    	</template>
    </data>
</odoo>