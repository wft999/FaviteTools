    <record  model="ir.model.fields"  id="favite_recipe_decode_mode">
            <field name="model_id" ref="favite_recipe.model_favite_recipe_decode" />
            <field name="name">x_mode</field>
			<field name="complete_name">decode.mode</field>
			<field name="ttype">selection</field>
            <field name="selection">[(0,'no decode'),(1,'glassid'),(2,'glass and panel id')]</field>
            <field name="default" eval="0" />
            
            <field name="locate">Global.</field>
            <field name="field_description">decode mode</field>
            <field name="help">it is a help</field>
            <field name="sequence">1</field>
    </record> 
